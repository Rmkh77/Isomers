class Solution(object):
    def isIsomorphic(self, s, t):
        # if len(set(s))!=len(set(t)):
        #     return False
        arr=[]
        lst=[]
        for idx in s:
            arr.append(s.index(idx))
        for idx in t:
            lst.append(t.index(idx))
        if arr==lst:
            return True
        return False
#beats 68%
